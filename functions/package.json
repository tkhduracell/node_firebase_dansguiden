{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext=jsx,ts,tsx src test lib debug",
    "test": "mocha -r ts-node/register --file src/setup.ts 'test/**/*.test.ts' --timeout 999999",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "backup": "ts-node debug/export.ts"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "google-gax": "^0.22.1",
    "grpc": "^1.23.3",
    "lodash": "^4.17.15",
    "lodash.orderby": "^4.6.0",
    "moment": "^2.22.2",
    "pug": "^2.0.4",
    "scraperjs": "^1.2.0",
    "simple-oauth2": "^1.5.0",
    "source-map-support": "^0.5.16",
    "spotify-web-api-node": "^4.0.0"
  },
  "private": true,
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/cheerio": "^0.22.16",
    "@types/lodash.orderby": "^4.6.6",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/simple-oauth2": "^2.5.2",
    "@types/spotify-api": "0.0.2",
    "@types/spotify-web-api-node": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^5.14.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^7.0.1",
    "nyc": "^14.1.1",
    "spotify-api": "0.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
