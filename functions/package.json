{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext=jsx,ts,tsx src test lib debug",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "script:backup": "ts-node scripts/export.ts",
    "script:events": "ts-node debug/events.ts",
    "script:metadata": "ts-node debug/metadata.ts",
    "script:band_search": "ts-node debug/band_search.ts"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.20.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "pug": "^3.0.2",
    "scraperjs": "^1.2.0",
    "simple-oauth2": "^5.0.0",
    "source-map-support": "^0.5.21",
    "spotify-web-api-node": "^5.0.2",
    "zod": "^3.19.1"
  },
  "overrides": {
    "google-gax": "^0.22.1",
    "grpc": "^1.23.3"
  },
  "private": true,
  "engines": {
    "node": "16"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/cheerio": "^0.22.31",
    "@types/express": "4.17.14",
    "@types/express-serve-static-core": "4.17.31",
    "@types/jest": "^29.2.3",
    "@types/lodash.orderby": "^4.6.7",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@types/simple-oauth2": "^4.1.1",
    "@types/spotify-api": "0.0.19",
    "@types/spotify-web-api-node": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@typescript-eslint/typescript-estree": "^5.43.0",
    "chai": "^4.3.7",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "nyc": "^15.1.0",
    "spotify-api": "0.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
